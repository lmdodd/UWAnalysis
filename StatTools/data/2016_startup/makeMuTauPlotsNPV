preselection='pt_1>24&&pt_2>20&&npv>=10&&npv<15&&diLeptons==0&&iso_1<0.1&&byTightIsolationMVArun2v1DBoldDMwLT_2>0.5&&tightMuons<=1&&tightElectrons==0&&againstMuonTight3_2>0&&againstElectronVLooseMVA6_2>0&&diLeptons==0'
